{
  "name": "kgt",
  "version": "1.0.0",
  "description": "Kitten Growth Tracker using MERN on AWS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktminks/KGT.git"
  },
  "keywords": [
    "health",
    "kitten",
    "cat",
    "growth",
    "tracker"
  ],
  "author": "Kathleen Minckler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ktminks/KGT/issues"
  },
  "homepage": "https://github.com/ktminks/KGT#readme",
  "proxy": "http://localhost:4000",
  "scripts": {
    "client": "cd ./front-end && npm build",
    "client:dev": "cd ./front-end && npm run start",
    "server": "cd ./back-end && npm start",
    "server:dev": "cd ./back-end && npm run start:dev",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "kill": "cd ./back-end && pm2 stop server.js"
  },
  "devDependencies": {
    "concurrently": "^6.2.1"
  }
}
